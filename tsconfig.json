// https://nitro.unjs.io/guide/typescript
{
  "extends": "./.nitro/types/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "lib": ["esnext", "dom", "dom.iterable"],
    "types": ["@cloudflare/workers-types"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
